{
  "name": "Upload asset to Watson Content Hub",
  "description": "Adds a context menu item that uploads selected image as an asset to Watson Content Hub",
  "version": "0.1",
  "permissions": ["contextMenus", "notifications", "storage", "https://*/*"],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "images/ico-16.png",
    "48": "images/ico-48.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "manifest_version": 2
}
